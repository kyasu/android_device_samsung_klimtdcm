# executeables
/(vendor|system/vendor)/bin/hw/vendor\.lineage\.touch@1\.0-service\.klimtdcm            u:object_r:hal_lineage_touch_default_exec:s0

# data files
/data/misc/akmd(.*)                                     u:object_r:hal_data_file:s0
/data/validity(/.*)?                                    u:object_r:vfsspi_data_file:s0

# device nodes
/dev/vfsspi                                             u:object_r:vfsspi_device:s0

# efs files
/efs/hw_offset                                          u:object_r:sensors_efs_file:s0

# sysfs
/sys/devices(/.*)?/input/input[2-4]/enabled             u:object_r:sysfs_hal_pwr:s0
/sys/devices/virtual/sec/sec_touchkey/keypad_enable     u:object_r:sysfs_sec_touchkey:s0

# sysfs - fingerprint
/sys/devices/virtual/fingerprint(/.*)?                  u:object_r:sysfs_fingerprint:s0
